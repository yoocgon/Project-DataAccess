<style>

  body {
  margin: 0;
  }

  .wrapper {
  display: grid;
  grid-template-areas:
  "stat"
  "menu"
  "head"
  "fter"
  "relt"
  "detl"
  "foot";
  grid-template-columns: 100%;
  grid-template-rows: 2em 4em 3em auto auto auto 2em;
  font-size: small;
  margin: 0%;
  padding: 0%;
  }

  .area-stat {
  grid-area: stat;
  /*border-bottom: 1px solid #CCC;*/
  /*border: 1px solid black;*/
  }

  .area-menu {
  grid-area: menu;
  display: flex;
  justify-content: space-between;
  background-color: #EEE;
  color: gray;
  border-bottom: 4px double #CCC;
  padding-top: 1.4em;
  padding-left: 2em;
  padding-right: 2em;
  color: #333;
  }

  .area-menu span:hover {
  font-weight: 800;
  color: black;
  animation: ani-select-menu 0.2s;
  font-size: larger;
  }

  @keyframes ani-select-menu {
  0% {
  font-size: small;
  }

  100% {
  font-size: larger;
  }
  }

  .area-header {
  grid-area: head;
  /*border: 1px dashed gray;*/
  }

  .area-header .title {
  font-size: medium;
  font-weight: 800;
  margin-left: 0.8em;
  color: black;
  padding-left: 2px;
  border-bottom: 1px solid black;
  }

  .area-filter {
  grid-area: fter;
  }

  .contianer-filter {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-top: 0.4em;
  margin-left: 0.8em;
  margin-right: 0.8em;
  padding: 1em;
  background: #333;
  color: white;
  border-radius: 3px;
  }

  .lb-filter {
  display: inline-block;
  width: 10em;
  margin-left: 1em;
  }

  #btn-search {
  float: right;
  margin: 1em;
  }

  .area-filter select {
  width: 12.7em;
  height: 1.4em;
  }

  .area-result {
  grid-area: relt;
  padding: 0.6em;
  /*border: 1px dashed gray;*/
  }

  #pagenation {
  float: right;
  }

  .btn-pagenation {
  display: inline-block;
  background: #CCC;
  margin: 2px;
  padding-bottom: 2px;
  width: 2em;
  border-radius: 4px;
  text-align: center;
  }

  .btn-pagenation-selected {
  display: inline-block;
  background: #333;
  color: white;
  margin: 2px;
  padding-bottom: 2px;
  width: 2em;
  border-radius: 4px;
  text-align: center;
  }

  #lb-title {
  margin-left: 1.2em;
  margin-bottom: 0;
  }

  #area-detail {
  grid-area: detl;
  padding: 1em;
  /*border: 1px dashed gray;*/
  }

  #contianer-detail {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin: 1em;
  padding: 1em;
  border: 1px double #DDD;
  }

  .lb-detail {
  display: inline-block;
  width: 10em;
  font-weight: 800;
  }

  .tx-detail {
  display: inline-block;
  width: 18em;
  }

  .area-footer {
  grid-area: foot;
  /*border: 1px dashed gray;*/
  /*border-top: 1px solid black;*/
  background-color: #777;
  color: silver;
  }

  #tb_result {
  width: 100%;
  background: white;
  font-size: small;
  font: normal 1px 100 verdana;
  }

  #tb_result th {
  background: #CCC;
  }

  #tb_result tr {
  border-bottom: 2px solid #EEE;
  background: #EEE;
  padding: 2px;
  }

  #tb_result tr:hover {
  background: #222;
  color: white;
  }

